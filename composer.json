{
  "name": "peterbenke/pb-rel-nofollow",
  "type": "typo3-cms-extension",
  "description": "Adds a rel-nofollow-attribute to extern links. You can exclude URLs by typoscript.",
  "keywords": [
    "TYPO3",
    "extension",
    "frontend",
    "links",
    "nofollow"
  ],
  "homepage": "https://www.typomotor.de",
  "authors": [
    {
      "name": "Peter Benke",
      "role": "Developer",
      "homepage": "https://www.typomotor.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": ">=8.1.0",
    "typo3/cms-core": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "PeterBenke\\PbRelNofollow\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "pb_rel_nofollow"
    }
  }
}